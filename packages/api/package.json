{
  "name": "@hono-aws-lambda/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "bun run src/dev.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "type-check": "tsc --noEmit",
    "lint": "bunx @biomejs/biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "hono": "^4.7.2",
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.4.1",
    "@hono/zod-openapi": "^1.1.6",
    "@scalar/hono-api-reference": "^0.6.0",
    "better-auth": "^1.0.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-kit": "^0.30.0",
    "postgres": "^3.4.5",
    "zod": "^4.2.1",
    "@aws-sdk/client-secrets-manager": "^3.722.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.722.0",
    "@aws-sdk/client-cloudwatch": "^3.722.0",
    "@aws-sdk/client-xray": "^3.722.0",
    "ioredis": "^5.4.1",
    "dompurify": "^3.2.2",
    "jsdom": "^25.0.1",
    "compression": "^1.7.5",
    "lru-cache": "^11.2.4",
    "langchain": "latest",
    "@langchain/core": "latest",
    "@langchain/langgraph": "latest",
    "@langchain/openai": "latest",
    "langsmith": "^0.4.0",
    "@hono-aws-lambda/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/compression": "^1.7.5",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@types/aws-lambda": "^8.10.147",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
